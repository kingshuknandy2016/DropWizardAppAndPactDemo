{
    "provider": {
        "name": "currencyExchange_provider"
    },
    "consumer": {
        "name": "currencyExchange_Consumer_v2"
    },
    "interactions": [
        {
            "description": "GET REQUEST /currency-exchange/from/{from}/to/{to}",
            "request": {
                "method": "GET",
                "path": "/currency-exchange/from/USD/to/INR"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "from": "USD",
                    "id": 10001,
                    "to": "INR",
                    "exchangeEnvironmentInfo": " ENV 1",
                    "conversionMultiple": 65
                },
                "matchingRules": {
                    "body": {
                        "$.id": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.from": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.to": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.conversionMultiple": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.exchangeEnvironmentInfo": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "The output of currency exchange service from 1 USD to 1 INR"
                }
            ]
        },
